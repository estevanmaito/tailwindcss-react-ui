{
  "name": "tailwindcss-react-ui",
  "version": "0.0.0-development",
  "description": "Tailwind CSS React UI components.",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:tailwind": "npx tailwindcss build style/tailwind.css -o style/output.css",
    "predev": "npm run build:tailwind",
    "dev": "styleguidist server",
    "build:styleguide": "styleguidist build",
    "cz": "git-cz",
    "test": "jest",
    "codecov": "codecov",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prebuild": "rimraf dist",
    "build": "webpack",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/estevanmaito/tailwindcss-react-ui.git"
  },
  "keywords": [
    "tailwind",
    "react",
    "ui",
    "components"
  ],
  "author": "Estevan Maito <ejmaito@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/estevanmaito/tailwindcss-react-ui/issues"
  },
  "homepage": "https://github.com/estevanmaito/tailwindcss-react-ui#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "tailwindcss": ">=1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "babel-loader": "8.1.0",
    "codecov": "3.6.5",
    "commitizen": "4.1.2",
    "css-loader": "3.5.3",
    "cz-conventional-changelog": "3.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-styleguidist": "11.0.6",
    "rimraf": "3.0.2",
    "semantic-release": "^19.0.3",
    "style-loader": "1.2.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test:coverage && npm run build:styleguide"
    }
  },
  "dependencies": {
    "classnames": "2.2.6",
    "prop-types": "15.7.2"
  }
}
